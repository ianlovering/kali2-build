# search path for the c32 support libraries (libcom32, libutil etc.)
path debian-installer/amd64/boot-screens/

label build_kali_native
        menu label ^Build Kali2 for Native
        menu default
        kernel debian-installer/amd64/linux
        append auto=true url=http://build/config/preseed.cfg priority=critical vga=788 initrd=debian-installer/amd64/initrd.gz --- quiet

default debian-installer/amd64/boot-screens/vesamenu.c32
prompt 0
timeout 0

